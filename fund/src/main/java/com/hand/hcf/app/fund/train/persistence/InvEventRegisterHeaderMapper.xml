<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hand.hcf.app.fund.train.persistence.InvEventRegisterHeaderMapper">

    <select id="selectByInvNumber" resultType="com.hand.hcf.app.fund.train.domain.InvEventRegisterHeader">
        SELECT *
        FROM inv_event_register_header
        WHERE inv_number=#{invNumber}
    </select>

</mapper>